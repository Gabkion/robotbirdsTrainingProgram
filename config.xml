<?xml version='1.0' encoding='utf-8'?>
<widget id="com.examples.RBTest" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RobotBirds Training Program</name>
    <icon src="./www/img/icon.png" />
    <description>
        An app made for the GIA competitions from RobotBirds.
    </description>
    <author email="fll.robotbirds@gmail.com">
        RobotBirds
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
